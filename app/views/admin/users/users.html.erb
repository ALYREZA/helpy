<% title "#{admin_title} #{t(:users_found, count: @users.size, query: params[:q].titleize, default: 'Search Results') unless params[:q] == 'users'}" %>

<h2 id="ticket-page-title">
  <%= t(:users_found, count: @users.size, query: params[:q].titleize, default: 'Search Results') unless params[:q] == 'users'%> <%= status_label(@status) if @status %> <span class='hidden-xs pull-right'><%= link_to t(:open_new_discussion, default: 'Open Discussion'), new_admin_topic_path, remote: true, class: 'btn btn-primary' %></span><%= render 'admin/topics/ticket_nav_dropdown' %>
</h2>

<div class="row">
  <div id="tickets">
    <%= render partial: 'admin/users/users' %>
  </div>
</div>
